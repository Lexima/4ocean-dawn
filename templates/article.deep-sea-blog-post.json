    "dc0e5a24-b6a0-4b5d-91cd-ead5d57702d3": {
      "type": "image-with-text",
      "blocks": {
        "template--15981434405057__dc0e5a24-b6a0-4b5d-91cd-ead5d57702d3-16655121768db680bc-0": {
