        "166316308404ff769e-0": {
          "type": "heading",
          "settings": {
            "heading": "<p>Bracelet of the Month Subscriptions<\/p>",
            "heading_size": "h2"
          }
        },
        "166316308404ff769e-1": {
          "type": "text",
          "settings": {
            "text": "<p>Get a new, limited-edition awareness bracelet that represents an animal or ecosystem impacted by plastic pollution each month. <\/p>"
          }
        }
      },
