    "16631633950fb83178": {
      "type": "multicolumn",
      "blocks": {
        "1663163394586c9d1e-0": {
          "type": "column",
          "settings": {
            "title": "Affordable",
            "text": "<p>Pull up to 1,000 pounds of trash from the world’s oceans, rivers, and coastlines each month and pay just $10 per pound.<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "1663163394586c9d1e-1": {
          "type": "column",
          "settings": {
            "title": "Impact Only",
            "text": "<p>Pound+ is a service so you don’t have to purchase any products to pull pounds. Think of it like hiring a landscaper to tend your lawn except our crews clean the ocean for you.<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "1663163394586c9d1e-2": {
          "type": "column",
          "settings": {
            "title": "Digital Certificate",
            "text": "<p>Receive a digital certificate that verifies your monthly impact. Hang it up or share it on social media as a way to share our mission and inspire others to get involved.<\/p>",
            "link_label": "",
            "link": ""
          }
        }
      },
