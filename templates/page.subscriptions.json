    "16631625902b432627": {
      "type": "multicolumn",
      "blocks": {
        "16631625907c0d2399-0": {
          "type": "column",
          "settings": {
            "title": "Double Your Impact",
            "text": "<p>Pull not one but TWO pounds of trash from the world’s oceans, rivers, and coastlines with each bracelet you receive through your Bracelet of the Month Subscription.<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "16631625907c0d2399-1": {
          "type": "column",
          "settings": {
            "title": "Advanced Shipping",
            "text": "<p>New bracelets ship two weeks before they’re available online so you’ll always be the first to wear them. If you’re in the contiguous U.S., shipping is FREE!<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "16631625907c0d2399-2": {
          "type": "column",
          "settings": {
            "title": "Exclusive Discounts",
            "text": "<p>Get 10% OFF select products when you add them to upcoming subscription orders, plus early access to new products and sales.<\/p>",
            "link_label": "",
            "link": ""
          }
        }
      },
