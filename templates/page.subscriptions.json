    "1663164476c7d4b4f5": {
      "type": "rich-text",
      "blocks": {
        "16631644761577cf77-0": {
          "type": "heading",
          "settings": {
            "heading": "<p>Join the Clean Ocean Club<\/p>",
            "heading_size": "h1"
          }
        },
        "16631644761577cf77-1": {
          "type": "text",
          "settings": {
            "text": "<p>Our monthly subscriptions are the #1 way to fund our global cleanup operation and support our mission to end the ocean plastic crisis. Choose one of our Bracelet of the Month subscriptions or sign up for our impact-only Pound+ subscription instead.<\/p>"
          }
        },
        "16631644761577cf77-2": {
          "type": "button",
          "settings": {
            "button_label": "I want a new bracelet monthly",
            "button_link": "shopify:\/\/products\/clean-ocean-club-beaded-bracelet-subscription",
            "button_style_secondary": true,
            "button_label_2": "I want to pull pounds monthly",
            "button_link_2": "shopify:\/\/products\/pound-plus-subscription",
            "button_style_secondary_2": false
          }
        }
      },
